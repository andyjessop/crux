{
  "sourceRoot": "packages/sync-queue/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/sync-queue",
        "main": "packages/sync-queue/src/index.ts",
        "tsConfig": "packages/sync-queue/tsconfig.lib.json",
        "assets": ["packages/sync-queue/*.md"]
      }
    },
    "prod": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/sync-queue",
        "tsConfig": "packages/sync-queue/tsconfig.lib.json",
        "project": "packages/sync-queue/package.json",
        "entryFile": "packages/sync-queue/src/index.ts",
        "format": ["esm", "cjs"],
        "rollupConfig": [
          "rollup.lib.prod.js",
          "packages/sync-queue/rollup.lib.js"
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./node_modules/.bin/ts-node tools/scripts/packages/publish-single.ts sync-queue"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/sync-queue/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/sync-queue"],
      "options": {
        "jestConfig": "packages/sync-queue/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
