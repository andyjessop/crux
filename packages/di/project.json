{
  "root": "packages/di",
  "sourceRoot": "packages/di/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/di",
        "main": "packages/di/src/index.ts",
        "tsConfig": "packages/di/tsconfig.lib.json",
        "assets": ["packages/di/*.md"]
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mylib/tsconfig-prod.lib.json"
        }
      }
    },
    "prod": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/di",
        "tsConfig": "packages/di/tsconfig.lib.json",
        "project": "packages/di/package.json",
        "entryFile": "packages/di/src/index.ts",
        "format": [
          "esm",
          "cjs"
        ],
        "rollupConfig": ["rollup.lib.prod.js"]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./node_modules/.bin/ts-node tools/scripts/packages/publish-single.ts di"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/di/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/di"],
      "options": {
        "jestConfig": "packages/di/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
