{
  "root": "packages/test",
  "sourceRoot": "packages/test/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/test",
        "tsConfig": "packages/test/tsconfig.lib.json",
        "project": "packages/test/package.json",
        "entryFile": "packages/test/src/index.ts",
        "format": [
          "esm",
          "cjs"
        ],
        "rollupConfig": ["rollup.lib.js"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/test/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/test"],
      "options": {
        "jestConfig": "packages/test/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
