{
  "sourceRoot": "packages/redux-slice/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/redux-slice",
        "main": "packages/redux-slice/src/index.ts",
        "tsConfig": "packages/redux-slice/tsconfig.lib.json",
        "assets": ["packages/redux-slice/*.md"]
      }
    },
    "prod": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/redux-slice",
        "tsConfig": "packages/redux-slice/tsconfig.lib.json",
        "project": "packages/redux-slice/package.json",
        "entryFile": "packages/redux-slice/src/index.ts",
        "format": ["esm", "cjs"],
        "rollupConfig": [
          "rollup.lib.prod.js",
          "packages/redux-slice/rollup.lib.js"
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./node_modules/.bin/ts-node tools/scripts/packages/publish-single.ts redux-slice"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/redux-slice/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/redux-slice"],
      "options": {
        "jestConfig": "packages/redux-slice/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
